/*** set_interval.fla   frame 1, Actions layer ***/// variable declarationsvar loaded:Number;var total:Number;var percent:Number;var intervalID:Number;// function declarationsfunction progressTracker(levelStr:String):Void {    loaded = _root[levelStr].getBytesLoaded();    total  = _root[levelStr].getBytesTotal();    if (loaded == undefined || total == undefined || total < 10) {        return;    }    percent = Math.round(loaded/total * 100);    bytesLoaded_txt.text = Math.floor(loaded/1024);    bytesTotal_txt.text  = Math.floor(total/1024);    percentage_txt.text  = percent;    if (loaded == total && percent > 99) {        clearInterval(intervalID);    }}// event callback handlers// initial frame actionsloadMovieNum("large.swf", 1);intervalID = setInterval(progressTracker, 83, "_level1");this.swapDepths(2);               // swap text fields with contents of Level 2